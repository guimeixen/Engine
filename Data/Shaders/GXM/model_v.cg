#include "include/ubos.cgh"

uniform float4x4 modelMatrix;

void main(
	float3 inPos,
	float2 inUv,
	float3 inNormal,
	float4 out outPos : POSITION,
	float2 out outUv : TEXCOORD0,
	float3 out outNormal : TEXCOORD1)
{
	outPos = mul(float4(inPos, 1.0f), modelMatrix);
	outPos = mul(outPos, cam.projView);
	outUv = inUv;
	outNormal = inNormal;
}